# TEBSarvis Multi-Agent System Environment Configuration

# Azure OpenAI Configuration
AZURE_OPENAI_ENDPOINT=https://your-openai-resource.openai.azure.com/
AZURE_OPENAI_KEY=your-openai-api-key
AZURE_OPENAI_API_VERSION=2024-02-01
AZURE_OPENAI_CHAT_MODEL=gpt-4
AZURE_OPENAI_EMBEDDING_MODEL=text-embedding-ada-002
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4-deployment

# Azure Cosmos DB Configuration
COSMOS_DB_URL=https://your-cosmos-account.documents.azure.com:443/
COSMOS_DB_KEY=your-cosmos-primary-key
COSMOS_DB_DATABASE=tebsarvis
COSMOS_DB_CONTAINER=incidents

# Azure Cognitive Search Configuration
SEARCH_SERVICE_ENDPOINT=https://your-search-service.search.windows.net
SEARCH_API_KEY=your-search-admin-key
SEARCH_INDEX_NAME=incidents-index

# Azure Functions Configuration
AZURE_FUNCTIONS_WORKER_RUNTIME=python
FUNCTIONS_EXTENSION_VERSION=~4
FUNCTIONS_WORKER_PROCESS_COUNT=1

# Agent System Configuration
AGENT_REGISTRY_TIMEOUT=300
MESSAGE_BUS_TIMEOUT=30
MAX_CONCURRENT_AGENTS=20
AGENT_HEARTBEAT_INTERVAL=30

# Monitoring and Logging
LOG_LEVEL=INFO
APPLICATION_INSIGHTS_CONNECTION_STRING=your-app-insights-connection-string

# Security Configuration
FUNCTION_AUTH_LEVEL=Function
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# Performance Configuration
MAX_EMBEDDING_BATCH_SIZE=100
SEARCH_RESULT_CACHE_TTL=300
COSMOS_DB_REQUEST_TIMEOUT=30

# Multi-Agent System Configuration
MAX_AGENT_RETRIES=3
AGENT_TASK_TIMEOUT=300
COLLABORATION_SESSION_TIMEOUT=1800
WORKFLOW_EXECUTION_TIMEOUT=900

# External Integration
WEBHOOK_SECRET_KEY=your-webhook-secret
NOTIFICATION_EMAIL_ENDPOINT=your-email-service-endpoint
TEAMS_WEBHOOK_URL=your-teams-webhook-url